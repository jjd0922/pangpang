<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newper.mapper.ConsultationResultMapper">
    <select id="selectConsultationResultDatatable" resultType="Map" parameterType="Map">
        SELECT
        CR_IDX,
        CR_TITLE,
        CR_CONTENT,
        CR_TYPE,
        CREATED_DATE
        FROM
        consultation_result
        <!--        <where>
                    <include refid="includeTemplateFormDataTable"></include>
                </where>-->
        ORDER BY
        CR_IDX DESC
        <if test="length!=-1">
            LIMIT #{start},#{length}
        </if>
    </select>

    <select id="countConsultationResultDatatable" resultType="Integer">
        SELECT
        COUNT(*)
        FROM
        consultation_result
        <!--        <where>
                    <include refid="includeTemplateFormDataTable"></include>
                </where>-->
    </select>
</mapper>
