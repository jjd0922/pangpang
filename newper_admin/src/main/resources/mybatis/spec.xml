<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newper.mapper.SpecMapper">
    <select id="selectSpecListValueList" resultType="String">
        SELECT DISTINCT(SPECL_VALUE) FROM SPEC_LIST
        WHERE SPECL_NAME=#{speclName}
        ORDER BY SPECL_NAME
    </select>
</mapper>